{
  "name": "flash-loans-manager",
  "version": "0.1.0",
  "description": "",
  "repository": "https://github.com/eingress/node-es2018-boilerplate.git",
  "author": "elcitrovmtgrande <victor.mahe.pro@gmail.com>",
  "license": "ISC",
  "keywords": [
    "babel",
    "eslint",
    "jest",
    "node",
    "prettier"
  ],
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon src/index.js",
    "build": "yarn build:clean && babel src -d dist",
    "dev": "npx concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "build:clean": "rimraf dist/*",
    "build:watch": "yarn build -w",
    "coverage": "jest --collectCoverageFrom=src/**/*.js --coverage",
    "lint": "eslint './{,{__tests__,src}/**/}*.js' --fix --quiet",
    "prestart": "yarn build",
    "start": "node dist/index.js",
    "start:dev": "nodemon src/index.js",
    "test": "jest --watchAll"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.0",
    "@babel/eslint-parser": "7.13.14",
    "@babel/node": "7.13.13",
    "@babel/preset-env": "7.14.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "*",
    "cors": "^2.8.5",
    "dotenv": "^9.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "ethers": "^5.1.4",
    "express": "^4.17.1",
    "graphql-request": "^3.4.0",
    "joi": "^17.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "socket.io": "^4.0.2",
    "web-vitals": "^1.0.1"
  }
}
